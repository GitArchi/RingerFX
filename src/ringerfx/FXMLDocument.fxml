<AnchorPane id="AnchorPane" prefHeight="245.0" prefWidth="480.0" style="-fx-background-color: e3dfdc;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ringerfx.FXMLDocumentController">
    <children>
        <Button fx:id="buttonInSystem" layoutX="46.0" layoutY="194.0" onAction="#buttonActionEnter" onKeyPressed="#handleEnter" prefHeight="25.0" prefWidth="149.0" style="-fx-background-color: #678a99;" text="Вход" textFill="#f9f8ff">
         <font>
            <Font size="14.0" />
         </font></Button>
      <TextField fx:id="textFieldLogin" layoutX="34.0" layoutY="73.0" promptText="Логин">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Label fx:id="labelInSystem" layoutX="72.0" layoutY="28.0" text="Вход в систему" textFill="#4a6875">
         <font>
            <Font size="14.0" />
         </font></Label>
      <ImageView fx:id="imageViewPicLogo" fitHeight="203.0" fitWidth="218.0" layoutX="240.0" layoutY="21.0">
         <image>
            <Image url="@pic/logo.JPG" />
         </image>
      </ImageView>
      <PasswordField fx:id="textFieldPassword" layoutX="34.0" layoutY="131.0" onKeyPressed="#handleEnter" prefHeight="30.0" prefWidth="173.0" promptText="Пароль">
